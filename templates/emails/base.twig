<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" style="margin: 0;padding: 0;">
	<head style="margin: 0; padding: 0;">
		<!-- If you delete this meta tag, Half Life 3 will never be released. -->
		<meta name="viewport" content="width=device-width" style="margin: 0;padding: 0;">
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" style="margin: 0;padding: 0;">
		<title style="margin: 0;padding: 0;">{% block title %}Welcome to hackstack{% endblock %}</title>
	</head>

	<body bgcolor="#FFFFFF" style="margin: 0; padding: 0; -webkit-font-smoothing: antialiased; -webkit-text-size-adjust: none; height: 100%; width: 100%; font-family: 'Helvetica Neue', 'Helvetica', Helvetica, Arial, sans-serif;">
		<!-- HEADER -->
		<table class="head-wrap" bgcolor="#34495E" style="margin: 0;padding: 0;width: 100%;">
			<tr style="margin: 0;padding: 0;">
				<td style="margin: 0;padding: 0; width:0px;"></td>
				<td class="header container" style="margin: 0 auto;padding: 0;display: block;min-width: 600px;clear: both;">
					<div class="content" style="margin: 0 auto;padding: 5px;min-width: 600px;display: block;">
						<table style="margin: 0;padding: 0;width: 100%;">
							<tr style="margin: 0;padding: 0;">
								<td>
									{% block logo %}
										<h1 style="color: #FFF; margin:0px;">HackStack</h1>
									{% endblock %}
								</td>
							</tr>
						</table>
					</div>
				</td>
				<td style="margin: 0;padding: 0;"></td>
			</tr>
		</table><!-- /HEADER -->


		<!-- BODY -->
		<table class="body-wrap" style="margin: 0;padding: 0;width: 100%;">
			<tr style="margin: 0;padding: 0;">
				<td style="margin: 0;padding: 0;"></td>
				<td class="container" bgcolor="#FFFFFF" style="margin: 0 auto;padding: 0;display: block;min-width: 600px;clear: both;">
					<div class="content" style="margin: 0 auto;padding: 15px;min-width: 600px;display: block;">
						<table style="margin: 0;padding: 0;width: 100%;">
							<tr style="margin: 0;padding: 0;">
								<td style="margin: 0;padding: 0;">
									{% block content %}{% endblock %}
								</td>
							</tr>
						</table>
					</div>
				</td>
				<td style="margin: 0;padding: 0;"></td>
			</tr>
		</table><!-- /BODY -->

		<!-- FOOTER -->
		<table class="footer-wrap" bgcolor="#34495E" style="margin: 0;padding: 0;width: 100%;clear: both;">
			<tr style="margin: 0;padding: 0;">
				<td style="margin: 0;padding: 0;"></td>
				<td class="container" style="margin: 0 auto;padding: 0;display: block;min-width: 600px;clear: both;">

					<!-- content -->
					<div class="content" style="margin: 0 auto;padding: 0px;min-width: 600px;display: block;">
						<table style="margin: 0;padding: 0;width: 100%;">
							<tr style="margin: 0;padding: 0;">
								<td align="center" style="margin: 0;padding: 0;">
									<p style="margin: 0;padding: 0;margin: 2x 0px;font-weight: normal;font-size: 14px;line-height: 1.6;">&nbsp;</p>
								</td>
							</tr>
						</table>
					</div><!-- /content -->
				</td>
				<td style="margin: 0;padding: 0;"></td>
			</tr>
		</table><!-- /FOOTER -->
	</body>
</html>